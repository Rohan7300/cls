<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Celerity" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/light_orange</item>
        <item name="android:statusBarColor">@color/medium_orange</item>
        <item name="colorPrimary">@color/light_orange</item>

        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black0</item>

    </style>
    <style name="BottomNavigationViewTextStyle">

        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">11sp</item>

    </style>
<style name="ctanine" parent="Theme.MaterialComponents.NoActionBar">


    <item name="android:windowDisablePreview">true</item>
    <item name="colorOnPrimary">@color/white</item>
    <item name="android:windowBackground">@drawable/backsplash</item>
</style>
    <style name="BoxLinedLayout" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="boxBackgroundColor">@android:color/white</item>
        <item name="boxBackgroundMode">filled</item>
        <item name="boxStrokeColor">@color/black</item>
        <item name="boxStrokeWidthFocused">2dp</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="android:textColorHint">@color/stone_grey</item>
    </style>
    <style name="place_the_r">
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">#4A4A4A</item>
    </style>
    <style name="ctaStyle1" parent="Theme.AppCompat">

        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:fadeEnabled">true</item>
        <item name="android:background">@drawable/cta_button</item>
    </style>

    <style name="CustomDialog" parent="android:Theme.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>


</resources>